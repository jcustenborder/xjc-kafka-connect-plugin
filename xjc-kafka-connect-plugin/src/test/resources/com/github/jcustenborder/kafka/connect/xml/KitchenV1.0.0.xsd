            <xs:element name="MaximumLogHoldDays" type="xs:int">
                <xs:annotation>
                    <xs:documentation>The number of days that the logged history records are kept in
                        memory before they can be over written by subsequent record
                    logs</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="UtilityLogHistory"
                type="UtilityLogHistoryObjectGroupType">
                <xs:annotation>
                    <xs:documentation>Table of utility history log records within the hosted
                        equipment that shall be logged under utility management practices. Systems
                        that have multiple recording events will have more than one entry in this
                        table</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Enable" type="NAFEMChoice" use="required">
            <xs:annotation>
                <xs:documentation>YES enables history table activation.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="UtilityLogHistoryObjectGroupType">
        <xs:annotation>
            <xs:documentation>An entry in the table of history properties for each utility type
                logged.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Index" type="xs:int">
                <xs:annotation>
                    <xs:documentation>The index reference to the utility management
                    table.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="EntryIndex" type="xs:int">
                <xs:annotation>
                    <xs:documentation>The index into the utilityMgmtLogHistTable</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Value" type="NAFEMValue">
                <xs:annotation>
                    <xs:documentation>The value of the associated attribute defined by the NAME
                        fieldState that is recorded due to a triggered log event</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Timestamp" type="NAFEMDateTime">
                <xs:annotation>
                    <xs:documentation>The date and time that the value of the associated attribute
                        defined by the NAME fieldState was recorded due to a triggered log
                    event</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="UtilityMessageObjectGroupType">
        <xs:annotation>
            <xs:documentation>Entry into the utility notify table</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="NAFEMMessagesType"/>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="UtilityNotificationObjectGroupType">
        <xs:sequence>
            <xs:element name="UtilityMessage" type="UtilityMessageObjectGroupType">
                <xs:annotation>
                    <xs:documentation>Table of utility notify records within the hosted equipment
                        that shall be logged under utility management practices</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="UtilityAcknowledgement"
                type="UtilityAcknowledgeObjectGroupType">
                <xs:annotation>
                    <xs:documentation>Table of associated acknowledgement feature attribute to the
                        notification message. This table is used when the system requires a response
                        from a manager when a trap is sent</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="UtilityConfigurationTypeCode">
        <xs:union memberTypes="UtilityConfigurationTypeCodeEnumeration TypeCodeExtension"/>
    </xs:simpleType>
    <xs:simpleType name="UtilityConfigurationTypeCodeEnumeration">
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="NotAvailable"/>
            <xs:enumeration value="Gas"/>
            <xs:enumeration value="Electric"/>
            <xs:enumeration value="Water"/>
            <xs:enumeration value="Steam"/>
            <xs:enumeration value="CompressedGas"/>
            <xs:enumeration value="Refrigerant"/>
            <xs:enumeration value="Other"/>
        </xs:restriction>
    </xs:simpleType>
    <!--   -->
    <xs:complexType name="NAFEMAcknowledgementType">
        <xs:annotation>
            <xs:documentation>defines the parameters by which the manager must acknowledge recept of
                a notify message indicating an alarm behavior</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Index" type="xs:int">
                <xs:annotation>
                    <xs:documentation>Index for the rows of the
                    maintNotifyAckTable.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Interval" type="NAFEMInterval">
                <xs:annotation>
                    <xs:documentation>Delay interval between non-acknowledged trap events.
                        Applicable only when acknowledgement required.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Retries" type="xs:int">
                <xs:annotation>
                    <xs:documentation>Number of attempts of non-acknowledged trap events.
                        Applicable only when acknowledgement required.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Response" type="xs:anyURI">
                <xs:annotation>
                    <xs:documentation>Response word from the SNMP Manager to acknowledge.  Manager
                        writes his IP address to confirm receipt of notify message from
                    agent.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Acknowledge" type="NAFEMChoice">
            <xs:annotation>
                <xs:documentation>Acknowledgement required from manager, No Acknowledgement
                required</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="NAFEMMessagesType">
        <xs:annotation>
            <xs:documentation>Textual messages and a code</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Index">
                <xs:annotation>
                    <xs:documentation>Index into message notify table</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Message" type="NAFEMString32">
                <xs:annotation>
                    <xs:documentation>A textual description of the notify message</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Code" type="xs:int">
                <xs:annotation>
                    <xs:documentation>A numerical description of the notify
                    message</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Timestamp" type="NAFEMDateTime">
                <xs:annotation>
                    <xs:documentation>Date and Time at which an event occurred.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Enable" type="NAFEMChoice" use="required">
            <xs:annotation>
                <xs:documentation>1 = enable trap generation -- 2 = no trap
                generation</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
